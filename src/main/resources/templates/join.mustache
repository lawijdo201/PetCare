{{>layout/header}}
<h1>회원가입</h1>
    <div class="wrap">
        <form action="/loginPage/join" method="post">
            <h2>Join</h2>
            <h4>ID</h4>
            <input type="text" name="id" placeholder="아이디">
            <h4>e-mail</h4>
            <input type="email" name="email" placeholder="이메일">
            <h4>Password</h4>
            <input type="password" name="pw" placeholder="비밀번호">
            <button type="submit">회원가입</button>
        </form>
</div>
    </form>
{{>layout/footer}}
