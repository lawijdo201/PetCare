{{>layout/header}}
<h1>로그인 페이지</h1>
<div class="wrap">
    <div class="login">
        <h2>Log-in</h2>
        <div class="login_sns">
            <li><a href=""><i class="fab fa-instagram"></i></a></li>
            <li><a href=""><i class="fab fa-facebook-f"></i></a></li>
            <li><a href=""><i class="fab fa-twitter"></i></a></li>
        </div>
        <div class="login_id">
            <h4>ID</h4>
            <input type="email" name="" id="" placeholder="ID">
        </div>
        <div class="login_pw">
            <h4>Password</h4>
            <input type="password" name="" id="" placeholder="Password">
        </div>
        <div class="login_etc">
            <div class="checkbox">
                <input type="checkbox" name="" id=""> Remember Me?
            </div>
            <div class="forgot_pw">
                <a href="">Forgot Password?</a>
            </div>
        </div>
        <div class="submit">
            <input type="submit" value="로그인">
        </div>
        <div class="button">
            <input type="button" value="회원가입" onClick="location.href='http://localhost:8090/loginPage/joinPage'">
        </div>
    </div>
</div>
{{>layout/footer}}
